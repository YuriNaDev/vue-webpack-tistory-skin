<div class="sidebarElement tagbox">
	<h3>태그목록</h3>
	<div>
		<% if (isProd) { %>
		<s_random_tags>
			<a href="[##_tag_link_##]" class="[##_tag_class_##]">
				<a-tag>[##_tag_name_##]</a-tag>
			</a>
		</s_random_tags>
		<% } else { for (var i = 1; i <= 5; i++) { %>
		<a href="#" class="cloud<%= i %>">
			<a-tag>테스트<%= i %></a-tag>
		</a>
		<% } } %>
	</div>
</div>
