<s_rct_notice>
	<div class="sidebarElement recentNotice">
		<h3>RECENT NOTICE</h3>
		<ul class="list">
			<% if (isProd) { %>
			<s_rct_notice_rep>
				<li>
					<a href="[##_notice_rep_link_##]">
						[##_notice_rep_title_##]
					</a>
				</li>
			</s_rct_notice_rep>
			<% } else { for (var i = 1; i <= 3; i++) { %>
			<li><a href="#">공지글 <%= i %></a></li>
			<% } } %>
		</ul>
	</div>
</s_rct_notice>
