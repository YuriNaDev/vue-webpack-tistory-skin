<s_tag>
	<div class="inner-content taglog">
		<h2>태그 클라우드</h2>
		<div>
			<% if (isProd) { %>
			<s_tag_rep>
				<a href="[##_tag_link_##]" class="[##_tag_class_##]">
					<a-tag>[##_tag_name_##]</a-tag>
				</a>
			</s_tag_rep>
			<% } else { for (var i = 1; i <= 5; i++) { %>
			<a href="#" class="cloud<%= i %>">
				<a-tag>테스트<%= i %></a-tag>
			</a>
			<% } } %>
		</div>
	</div>
</s_tag>
